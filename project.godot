; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GDTerminal"
config/description="Run GDScript in the editor at the click of a button."
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.png"

[audio]

driver/driver="Dummy"

[display]

window/size/viewport_width=262
window/size/viewport_height=438
window/energy_saving/keep_screen_on=false

[editor_plugins]

enabled=PackedStringArray("res://addons/GDTerminal/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
